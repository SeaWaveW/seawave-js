{
    "presets": [
      ["@babel/preset-env", {
        "useBuiltIns": "usage",
        "modules": false,
        "targets": {
          "browsers": "last 2 versions, not ie <= 9"
        },
        "corejs":{
            "version":3 //core-js的版本
        }
      }]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime", {
        "helpers": false,
      }],
      "@babel/plugin-proposal-class-properties"
    ]
}